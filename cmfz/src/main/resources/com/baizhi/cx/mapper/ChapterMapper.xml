<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cx.mapper.ChapterMapper">

    <insert id="addChapter" parameterType="com.baizhi.cx.entity.Chapter">
insert into chapter(title,size,duration,chpath,uploaddate,albumid)
values(#{title},#{size},#{duration},#{chpath},NOW(),#{albumid})
    </insert>

</mapper>
